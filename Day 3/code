console.log(typeof 123);
console.log(typeof true);
console.log(typeof `abcd`);
console.log(typeof "abcd");
console.log(typeof 'abcd');

console.log(2<<1); //left shift operator

console.log(4>>1); //right shift operator

console.log(2 & 3); //bit wise and 

console.log(2 | 3); // bit wise or 

console.log(~3); // bit wise not
